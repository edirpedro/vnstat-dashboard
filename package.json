{
  "name": "vnstat-dashboard",
  "version": "0.2.1",
  "url": "https://github.com/edirpedro/vnstat-dashboard",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/d3": "^7.4.0",
    "@types/jest": "^29.5.1",
    "@types/luxon": "^3.3.0",
    "@types/node": "^20.1.3",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "billboard.js": "^3.5.1",
    "d3": "^7.6.1",
    "luxon": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.4",
    "react-scripts": "5.0.1",
    "sass": "^1.55.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:release": "bash tasks/release.sh",
    "build:docs": "bash tasks/docs.sh",
    "build:local": "bash tasks/local.sh",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "profile": "npm run build -- --stats && npx webpack-bundle-analyzer ./build/bundle-stats.json"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^d3$|^d3-": "<rootDir>/node_modules/d3/dist/d3.min.js"
    }
  }
}
